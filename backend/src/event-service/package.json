{
  "name": "@tribe/event-service",
  "version": "1.0.0",
  "description": "Event discovery, recommendation, and management service for the Tribe platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "npm run clean && npm run build",
    "docker:build": "docker build -t tribe/event-service .",
    "docker:run": "docker run -p 3003:3003 tribe/event-service"
  },
  "keywords": [
    "tribe",
    "events",
    "discovery",
    "recommendations",
    "microservice"
  ],
  "author": "Tribe Development Team",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@prisma/client": "^4.16.0",
    "@tribe/shared": "1.0.0",
    "axios": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "node-cache": "^5.1.2",
    "prom-client": "^14.2.0",
    "redis": "^4.6.7",
    "uuid": "^9.0.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.16.0",
    "rimraf": "^6.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}