{
  "name": "@tribe/profile-service",
  "version": "1.0.0",
  "description": "Profile service for the Tribe platform handling user profiles, personality traits, interests, and location data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "tribe",
    "profile",
    "personality",
    "interests",
    "microservice"
  ],
  "author": "Tribe Team",
  "license": "UNLICENSED",
