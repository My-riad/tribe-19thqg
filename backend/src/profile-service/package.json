    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.7.0",
    "joi": "^17.9.2",
    "uuid": "^9.0.0",
    "dotenv": "^16.3.1",
    "geolib": "^3.3.4",
    "winston": "^3.9.0",
    "morgan": "^1.10.0",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "date-fns": "^2.30.0",
