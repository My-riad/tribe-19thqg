{
  "name": "payment-service",
  "version": "1.0.0",
  "description": "Payment processing microservice for the Tribe platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "docker:build": "docker build -t tribe/payment-service .",
    "docker:run": "docker run -p 3005:3005 tribe/payment-service"
  },
  "keywords": [
    "payment",
    "microservice",
    "tribe",
    "stripe",
    "venmo",
    "split-payments"
  ],
  "author": "Tribe Development Team",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@prisma/client": "^4.16.0",
    "@tribe/shared": "1.0.0",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.0",
    "bull": "^4.10.4",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-prom-bundle": "^6.6.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "prom-client": "^14.2.0",
    "rate-limit-redis": "^3.0.0",
    "stripe": "^12.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/compression": "^1.7.2",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.4",
    "@types/node": "^22.13.14",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.0",
    "nock": "^13.3.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.16.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}